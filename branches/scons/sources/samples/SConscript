import os
Import('env', 'fae')

envsam = env.Copy()
envsam.Append(
	CPPDEFINES = [('funs', 'rasamples'), 'RASCAL_HELPERS'],
	category = 'smp',
	CPPPATH = ['../common/util'],
	LIBPATH = ['../' + env['LIBARCH']],
	LIBS = ['rascal'])

envsam.Program('raconn', fae.compile(envsam, ['../common/util', '01-raconn']))
envsam.Program('raconns', fae.compile(envsam, ['../common/util', '02-raconns']))
envsam.Program('rahost', fae.compile(envsam, ['../common/util', '04-rahost']))
envsam.Program('raflood', fae.compile(envsam, ['../common/util', '05-raflood']))
envsam.Program('ralc', fae.compile(envsam, ['../common/util', '06-ralc']))
envsam.Program('rasvc', fae.compile(envsam, ['../common/util', '07-rasvc']))

